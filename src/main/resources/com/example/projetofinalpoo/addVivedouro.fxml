<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projetofinalpoo.AddVivedouroController">
   <children>
      <TextField fx:id="campoVivedouro" layoutX="314.0" layoutY="313.0" prefHeight="30.0" prefWidth="211.0" promptText="Tipo do Vivedouro">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <TextField fx:id="campoTamanho" layoutX="723.0" layoutY="313.0" prefHeight="30.0" prefWidth="164.0" promptText="Tamanho do vivedouro">
         <font>
            <Font size="16.0" />
         </font>
         <cursor>
            <Cursor fx:constant="TEXT" />
         </cursor></TextField>
      <Button fx:id="salvarDados" layoutX="676.0" layoutY="581.0" mnemonicParsing="false" onAction="#onClick" text="Cadastrar">
         <font>
            <Font size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="voltar" layoutX="177.0" layoutY="581.0" mnemonicParsing="false" onAction="#voltarVivedouro" text="+">
         <font>
            <Font size="24.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Button>
      <Label layoutX="113.0" layoutY="314.0" text="TIPO VIVEDOURO">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="593.0" layoutY="314.0" text="TAMANHO">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="212.0" layoutY="79.0" text="Cadastrar Vivedouro">
         <font>
            <Font name="Bauhaus 93" size="64.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
